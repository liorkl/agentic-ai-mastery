{
  "name": "coach",
  "description": "Coaches developers to master agentic AI development. Assesses skill by scanning your Claude Code/Cowork environment, teaches from an 11-level curriculum, tracks progress, discovers new features.",
  "version": "1.0.0",
  "author": {
    "name": "Lior Klibansky"
  },
  "permissions": {
    "read": [
      "~/.claude/**",
      ".claude/**",
      "CLAUDE.md",
      "~/.claude/coaching/state/**"
    ],
    "write": [
      "~/.claude/coaching/**"
    ],
    "network": [
      "github.com",
      "anthropic.com"
    ],
    "bash": [
      "stat",
      "claude --version",
      "wc",
      "ls"
    ],
    "deny": [
      ".env*",
      "**/credentials*",
      "**/*.key",
      "**/*.pem",
      "**/secrets*"
    ]
  }
}
